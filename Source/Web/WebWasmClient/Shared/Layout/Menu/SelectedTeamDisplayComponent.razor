<div class="CurrentTenantDisplayComponent">
    <div class="LogoNameSection">
        @if(Team != null)
        {
            <img src="@Team.IconUrl"/>
            <h4>@Team.Name</h4>
            @if(User.IsInRole("Admin"))
            {
                <div class="RoleDisplay"> 
                    <h5>Admin</h5>
                </div>
            }
        }       
    </div>  
</div>

